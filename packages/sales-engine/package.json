{
  "name": "@parallelo/sales-engine",
  "version": "0.1.0",
  "description": "Domain library for conversational AI sales engine",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./llm": {
      "import": "./dist/llm/index.js",
      "types": "./dist/llm/index.d.ts"
    },
    "./media": {
      "import": "./dist/media/index.js",
      "types": "./dist/media/index.d.ts"
    },
    "./escalation": {
      "import": "./dist/escalation/index.js",
      "types": "./dist/escalation/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "sales",
    "ai",
    "conversational",
    "llm",
    "chatbot"
  ],
  "author": "Parallelo",
  "license": "MIT",
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@google/generative-ai": "^0.24.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "openai": "^6.9.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.10.0",
    "@google/generative-ai": ">=0.2.0",
    "openai": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@google/generative-ai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "dependencies": {
    "assemblyai": "^4.19.0"
  }
}
